{
    "name": "arhinful/laravel-mnotify",
    "description": "Laravel package for MNotify SMS and voice notifications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arhinful Emmanuel",
            "email": "kofibusy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Arhinful\\LaravelMnotify\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arhinful\\LaravelMnotify\\MNotifyServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://github.com/arhinful/laravel-mnotify",
    "repository": {
        "type": "vcs",
        "url": "https://github.com/arhinful/laravel-mnotify"
    },
    "keywords": [
        "laravel",
        "sms",
        "mnotify",
        "notification",
        "voice"
    ]
}